{
    "name": "facturasbackend",
    "version": "1.0.0",
    "description": "Back-end para carga de facturas. Incluye conexion con Business One y Document Information Extraction",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@cap-js/hana": "^1",
        "@sap-cloud-sdk/http-client": "^3.21.0",
        "@sap-cloud-sdk/util": "^3.21.0",
        "@sap/cds": "^8",
        "@sap/cds-sqlite": "^1.24.1",
        "@sap/low-code-event-handler": "^1.2.4",
        "@sap/mdk-tools": "^1.3.1",
        "@sap/xsenv": ">=3.3.2 <3.3.3",
        "@sap/xssec": "^4.2.4",
        "express": "^4",
        "hdb": "^0.19.9",
        "passport": "^0.7.0",
        "test": "*"
    },
    "workspaces": [
        "test"
    ],
    "devDependencies": {
        "@cap-js/cds-types": "^0.6",
        "@cap-js/sqlite": "^1.7.3",
        "@sap/dev-cap-tools": "^1.31.0",
        "axios": "^1.7.7",
        "chai": "^4.5.0",
        "chai-as-promised": "^7.1.2",
        "chai-subset": "^1.6.0",
        "jest": "^29.7.0",
        "sqlite3": "^5.1.7",
        "test": "*"
    },
    "scripts": {
        "start": "cds-serve",
        "start-local": "npm run generate-entry-point && npm run cds-watch",
        "generate-entry-point": "dev-cap-tools gen-entrypoint",
        "cds-watch": "cds watch --open",
        "test": "jest"
    },
    "cds": {
        "sql": {
            "native_hana_associations": false
        },
        "requires": {
            "db": {
                "[development]": {
                    "kind": "sqlite",
                    "impl": "@cap-js/sqlite",
                    "credentials": {
                        "url": "db.sqlite"
                    }
                }
            },
            "[production]": {
                "db": "hana",
                "auth": "xsuaa"
            },
            "app-service": {
                "impl": "@sap/low-code-event-handler"
            },
            "auth": {
                "[development]": {
                    "kind": "dummy"
                },
                "[production]": {
                    "kind": "xsuaa"
                }
            }
        },
        "features": {
            "fetch_csrf": true
        },
        "cap_dox_key": {
            "endpoints": {
                "backend": {
                    "url": "https://aiservices-dox.cfapps.us10.hana.ondemand.com"
                }
            },
            "swagger": "/document-information-extraction/v1/"
        }
    },
    "lcap": true
}